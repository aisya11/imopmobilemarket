<resources>


    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="windowActionBar">true</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppThemeActionBar" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="Material.Dark" parent="Theme.AppCompat.NoActionBar" >
        <item name="colorPrimary">@color/colorPrimary</item>
    </style>

    <style name="DarkActionBaru" parent="Theme.AppCompat.Light.DarkActionBar" >
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
    </style>

    <style name="Tes" parent="Theme.AppCompat.NoActionBar" >
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
    </style>

    <style name="detailBrgKiri">
        <!-- Customize your theme here. -->
        <item name="android:background">@drawable/border_solid</item>
        <item name="android:padding">5dp</item>
    </style>
    <style name="detailBrgKanan">
        <!-- Customize your theme here. -->
        <item name="android:background">@drawable/border_no</item>
        <item name="android:padding">5dp</item>
    </style>

</resources>
